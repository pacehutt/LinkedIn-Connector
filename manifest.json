{
    "name": "LinkedIn Auto Connect",
    "version": "1.0",
    "description": "Clicks 'Connect' button for every result on LinkedIn search page",
    "manifest_version": 3,
   
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/search/results/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "index.html",
      "default_title": "LinkedIn Auto Connect",
      "default_icon": "linked.png"
    }
  }
  